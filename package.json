{
  "name": "egghead",
  "version": "0.1.2",
  "description": "Simple quiz app",
  "main": "index.js",
  "scripts": {
    "build": "flatpak-builder --force-clean --build-only build-dir io.github.josephmawa.Egghead.json",
    "finish": "flatpak-builder --no-appstream-compose --force-clean --finish-only build-dir io.github.josephmawa.Egghead.json",
    "export": "flatpak-builder --force-clean --export-only --repo=repo build-dir io.github.josephmawa.Egghead.json",
    "start": "flatpak-builder --run build-dir io.github.josephmawa.Egghead.json io.github.josephmawa.Egghead",
    "clean": "rm -rf .flatpak-builder build-dir builddir repo"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/josephmawa/Egghead.git"
  },
  "keywords": [
    "quiz",
    "recreation",
    "app",
    "desktop",
    "linux"
  ],
  "author": "Joseph Mawa",
  "license": "GPL-3.0-or-later",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/josephmawa/Egghead/issues"
  },
  "homepage": "https://github.com/josephmawa/Egghead#readme"
}
