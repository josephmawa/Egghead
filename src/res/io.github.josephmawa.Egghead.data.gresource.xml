<?xml version="1.0" encoding="UTF-8"?>
<gresources>
  <!-- ui -->
  <gresource prefix="/io/github/josephmawa/Egghead">
    <!-- This resource must be at gtk/help-overlay for Gtk to associate it with the 
    current GtkApplicationWindow. Check https://docs.gtk.org/gtk4/class.Application.html -->
    <file preprocess="xml-stripblanks" compressed="true" alias="gtk/help-overlay.ui">ui/help-overlay.ui</file>
    <file preprocess="xml-stripblanks" compressed="true" alias="window.ui">ui/window.ui</file>
    <file preprocess="xml-stripblanks" compressed="true" alias="preferences.ui">ui/preferences.ui</file>
    <file preprocess="xml-stripblanks" compressed="true" alias="page.ui">ui/page.ui</file>
  </gresource>
  <!-- styles -->
  <gresource prefix="/io/github/josephmawa/Egghead">
    <file compressed="true" alias="index.css">styles/index.css</file>
  </gresource>
  <!-- icons -->
  <gresource prefix="/io/github/josephmawa/Egghead/icons">
    <file preprocess="xml-stripblanks" compressed="true" alias="scalable/actions/egghead-system-search-symbolic.svg">icons/egghead-system-search-symbolic.svg</file>
    <file preprocess="xml-stripblanks" compressed="true" alias="scalable/actions/egghead-open-menu-symbolic.svg">icons/egghead-open-menu-symbolic.svg</file>
    <file preprocess="xml-stripblanks" compressed="true" alias="scalable/actions/egghead-sidebar-show-right-symbolic-rtl.svg">icons/egghead-sidebar-show-right-symbolic-rtl.svg</file>
    <file preprocess="xml-stripblanks" compressed="true" alias="scalable/actions/egghead-go-first-symbolic-rtl.svg">icons/egghead-go-first-symbolic-rtl.svg</file>
    <file preprocess="xml-stripblanks" compressed="true" alias="scalable/actions/egghead-go-first-symbolic.svg">icons/egghead-go-first-symbolic.svg</file>
    <file preprocess="xml-stripblanks" compressed="true" alias="scalable/actions/egghead-go-next-symbolic-rtl.svg">icons/egghead-go-next-symbolic-rtl.svg</file>
    <file preprocess="xml-stripblanks" compressed="true" alias="scalable/actions/egghead-go-next-symbolic.svg">icons/egghead-go-next-symbolic.svg</file>
    <file preprocess="xml-stripblanks" compressed="true" alias="scalable/actions/egghead-object-select-symbolic.svg">icons/egghead-object-select-symbolic.svg</file>
    <file preprocess="xml-stripblanks" compressed="true" alias="scalable/actions/egghead-user-trash-symbolic.svg">icons/egghead-user-trash-symbolic.svg</file>
  </gresource>
</gresources>